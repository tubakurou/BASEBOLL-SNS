<% if user_signed_in? && user != current_user %>
    <td id="follow_form">
       <% if current_user.following?(user) %>
           <%= link_to "フォロー外す", user_relationships_path(user),remote: true,method: :delete %><%= user.followers.count %>
        <% else %>
            <%= link_to "フォローする", user_relationships_path(user),remote: true, method: :post %><%= user.followings.count %>
        <% end %>
     </td>
<% end %>