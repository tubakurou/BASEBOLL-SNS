<h2>Results index</h2>

<table class="table table-hover table-inverse">
  <% if @range == "User" %>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.name %></td>
        </tr>
      <% end %>
    </tbody>
  <% else %>
    <tbody>
      <% @game_comment.each do |game_comment| %>
        <tr>
          <td><%= game_comment.game_content %></td>
        </tr>
      <% end %>
    </tbody>
  <% end %