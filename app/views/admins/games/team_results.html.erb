<div class='container px-5 px-sm-0'>
  <h2>チーム成績手一覧</h2>
  <div class="row">
    <div class="col-xs-10 col-xs-offset-1">
      <table>
        <thead>
          <tr class="info">
            <th>イニング</th>
            <th>安打</th>
            <th>得点</th>
            <th>エラー</th>
          </tr>
        </thead>
        <tbody>
  				<% @team_results.each do |team_results| %>
    				<tr>
    				  <td class="info col-xs-1">
    				    <%= team_results.inning %>
    				  </td>
    				  <td class="info col-xs-1">
    				    <%= team_results.hit %>
    				  </td>
              <td class="info col-xs-1">
                <%= team_results.score %>
              </td>
                <td class="info col-xs-1">
    				    <%= team_results.error %>
    				  </td>
          <% end %>
            </tr>
          </tbody>
      </table>
    </div>
  </div>
</div>
